{
  "name": "prototype-boilerplate",
  "version": "1.0.0",
  "description": "Small Boilerplate for quick prototyping: HTML / CSS / JS ready to go",
  "main": "src/script/main.js",
  "author": "Tim Roussilhe",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.3",
    "babel": "^6.23.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.0",
    "eslint-config-google": "^0.9.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.28.0",
    "now": "^7.4.0",
    "style-loader": "^0.18.2",
    "underscore": "^1.8.3",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.4.2",
    "node-sass": "^4.5.2",
    "sass-loader": "^6.0.3"
  },
  "scripts": {
    "lint": "eslint ./src/js/**/*.js; exit 0",
    "start": "webpack-dev-server --hot --content-base ./public/ --open --quiet",
    "build": "npm run build-webpack && npm run deploy",
    "build-webpack": "ENV_PROD='production' webpack ",
    "deploy": "now public/"
  }
}
